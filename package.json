{
  "name": "drdocu",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "start": "nodemon src/index.ts",
    "start:raw": "node --enable-source-maps --experimental-json-modules --no-warnings dist/src/index.js",
    "build": "rm -rf dist && tsc && npm run start:raw",
    "pm2": "pm2 start assets/pm2/ecosystem.config.js",
    "upgrade": "rm -rf package-lock.json && npm cache clean --force && npm install && npm up && npm up -R",
		"format": "yarn prettier . --write",
		"format:check": "yarn prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsNevis/DrDocu.git"
  },
  "author": "Nevis",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/IsNevis/DrDocu/issues"
  },
  "homepage": "https://github.com/IsNevis/DrDocu#readme",
  "dependencies": {
    "chalk": "4.1.2",
    "discord.js": "^13.7.0",
    "strip-ansi": "^6.0.0",
    "wokcommands": "^1.5.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
